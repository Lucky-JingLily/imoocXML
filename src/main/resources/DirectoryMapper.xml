<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="directory">
    <insert id="insert">
      insert into directory(parent,inodes) values(#{parent},#{inodes})
    </insert>

    <select id="selectAll">
        select * from directory
    </select>
</mapper>